{
    "builds": [
        {
            "src": "stalkea_clone/server.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "stalkea_clone/server.py"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/phishing-check",
            "schedule": "*/15 * * * *"
        }
    ]
}